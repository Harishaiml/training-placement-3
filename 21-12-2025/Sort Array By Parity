class Solution {
    public int[] sortArrayByParity(int[] nums) {
        int n = nums.length;
        int low=0;
        int mid=0;

        while(mid<nums.length){
            if(nums[mid]%2==0){
                int temp=nums[mid];
                nums[mid]=nums[low];
                nums[low]=temp;


                low++;
            }
            mid++;
        }
        return nums;
    }
}

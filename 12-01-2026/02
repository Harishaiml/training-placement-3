import java.util.Scanner;

class Armstrong {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num, temp, sum = 0, r;

        num = sc.nextInt();
        temp = num;

        while (temp != 0) {
            r = temp % 10;
            sum += r * r * r;
            temp /= 10;
        }

        if (sum == num)
            System.out.println("Armstrong");
        else
            System.out.println("Not Armstrong");
    }
}
import java.util.Scanner;

class CountDigits {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num, count = 0;

        System.out.print("Enter number: ");
        num = sc.nextInt();

        while (num != 0) {
            count++;
            num = num / 10;
        }

        System.out.println("Total digits: " + count);
        sc.close();
    }
}